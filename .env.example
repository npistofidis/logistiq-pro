# Google Gemini API Key (Required)
# Get from: https://makersuite.google.com/app/apikey
GOOGLE_API_KEY=your_gemini_api_key_here

# Google Maps API Key (Optional - for route optimization)
# Get from: https://console.cloud.google.com/
GOOGLE_MAPS_API_KEY=your_maps_api_key_here

# OpenWeather API Key (Optional - for weather monitoring)
# Get from: https://openweathermap.org/api
OPENWEATHER_API_KEY=your_weather_api_key_here

# LangSmith (Optional - for agent tracing and evaluation)
# Get from: https://smith.langchain.com/
LANGCHAIN_TRACING_V2=false
LANGCHAIN_API_KEY=your_langsmith_api_key_here
LANGCHAIN_PROJECT=logistiq-pro

# Database Configuration
DATABASE_PATH=./logistiq_warehouse.db
CHROMA_DB_PATH=./chroma_db

# Application Settings
DEBUG=false
LOG_LEVEL=INFO
